{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "src/app.js" }],
    "routes": [
      {
        "src": "/api/v1/user/signin",
        "methods": ["POST"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }
  